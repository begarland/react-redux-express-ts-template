{
  "name": "react-redux-express-ts-template",
  "version": "3.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/begarland/react-redux-express-template"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "babel-plugin-transform-decorators-legacy"
    ]
  },
  "dependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "20.0.8",
    "@types/jquery": "^3.3.29",
    "@types/node": "^8.10.39",
    "@types/react": "16.0.18",
    "@types/react-dom": "16.0.2",
    "@types/react-router-redux": "5.0.8",
    "@types/redux-logger": "3.0.5",
    "@types/redux-promise-middleware": "0.0.8",
    "@types/sinon": "^5.0.7",
    "axios": "0.17.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "date-input-polyfill": "^2.14.0",
    "envify": "^4.1.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "express": "^4.16.4",
    "express-react-views": "^0.10.5",
    "history": "^4.6.3",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.3.2",
    "jest": "^22.4.4",
    "jquery": "3.2.1",
    "localenvify": "^1.0.1",
    "moment": "2.19.3",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "pg": "^7.8.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "redux-promise-middleware": "^4.4.2",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "rxios": "github:begarland/rxios",
    "rxjs": "^6.3.3",
    "rxjs-compat": "^6.3.3",
    "sinon": "^4.5.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.1",
    "tsify": "^4.0.1",
    "tslint": "^5.12.1",
    "typeorm": "^0.2.11",
    "typescript": "^2.9.2",
    "uglifyify": "^4.0.5",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.11.0",
    "winston": "^2.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).tsx?(x)"
    ],
    "moduleDirectories": [
      "node_modules"
    ]
  },
  "scripts": {
    "dev": "concurrently --raw \"npm run watch-server\"  \"gulp --silent\"",
    "lint": "node_modules/.bin/tslint 'src/**/*.ts' --force",
    "prod-build": "npm run production-sass; npm run production-web",
    "production-sass": "sass --sourcemap=none web/src/main.scss:web/public/main.css",
    "production-web": "node_modules/.bin/browserify web/src/index.tsx -p tsify -g [ envify --NODE_ENV production ] -g [ uglifyify ] -o web/public/bundle.js -v",
    "start": "node_modules/.bin/ts-node server/index.ts; npm run prod-build",
    "test": "jest --color --verbose --expand --watchAll",
    "watch-server": "cross-env NODE_ENV=development node_modules/.bin/nodemon --exec 'concurrently --raw \"npm run lint\" \"node_modules/.bin/ts-node server/index.ts\"'"
  },
  "devDependencies": {
    "ansi-colors": "^3.2.3",
    "browser-sync": "^2.26.3",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-plumber": "^1.2.1",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "node-notifier": "^5.3.0",
    "path": "^0.12.7"
  }
}
